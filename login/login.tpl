<main>
	<h2>Login</h2>
	<form id="login" method="post" action="[var.afurl.base]/login/auth"><div>
		<input type="text" name="auth_account"
			placeholder="Account" />
		<br />
		<input type="password" name="auth_password"
			placeholder="Password" />
		<br />
		<input type="submit" value="Login" />
	</div></form>
</main>
